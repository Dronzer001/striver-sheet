class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int n=nums.size();
        vector<int>mid;
        for(int i=0;i<n;i+=1){
            int key=target-nums[i];
            auto it=find(mid.begin(),mid.end(),key);
            if(it!=mid.end()){
                int index=it-mid.begin();
                return {i,index};
            }
            mid.push_back(nums[i]);
        }
        return {};
    }
};
