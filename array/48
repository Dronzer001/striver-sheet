class Solution {
public:
    void rotate(vector<vector<int>>& matrix) {
        //transpose and reverse
        int n=matrix.size();
        for(int i=0;i<n;i+=1){
            for(int j=0;j<n;j+=1){
                if(j>i){
                    swap(matrix[i][j],matrix[j][i]);
                }
            }
        }
        for(int i=0;i<n;i+=1){
            reverse(matrix[i].begin(),matrix[i].end());
        }
    }
};
