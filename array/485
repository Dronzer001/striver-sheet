class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int n=nums.size();
        int count=0,ans=0,i=0;
        while(i<n){
            while(i<n && nums[i]==1){
                i+=1;
                count+=1;
            }
            ans=max(ans,count);
            count=0;
            i+=1;
        }
        return ans;
    }
};
