class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int n=nums.size();
        int start=0,i=0;
        while(i<n && nums[i]==0) i+=1;
        while(i<n){
            if(i<n && nums[i]!=0){
                swap(nums[i],nums[start]);
                start+=1;
                i+=1;
            }
            else{
                i+=1;
            }
        }
    }
};
